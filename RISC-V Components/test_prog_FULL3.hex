# --- SETUP ---
addi x1, x0, -1      # x1 = 0xFFFFFFFF (-1)
addi x2, x0, 1       # x2 = 0x00000001 (1)

# --- PART 1: MEMORY HALF-WORD ---
sh   x1, 4(x0)       # Store Half: Writes 0xFFFF to Mem[4].
lh   x5, 4(x0)       # Load Half Signed: Should be 0xFFFFFFFF (-1).
lhu  x6, 4(x0)       # Load Half Unsigned: Should be 0x0000FFFF (65535).

# --- PART 2: R-TYPE LOGIC ---
sll  x7, x2, x2      # Shift Left: 1 << 1 = 2.
slt  x8, x1, x2      # Set Less Than (Signed): -1 < 1? YES (1).
srl  x9, x1, x2      # Shift Right Logic: -1 >> 1 = 0x7FFFFFFF (Positive).
sra  x10, x1, x2     # Shift Right Arith: -1 >>> 1 = 0xFFFFFFFF (-1).
xor  x11, x1, x2     # XOR: 111...11 ^ 000...01 = 111...10 (-2).

# --- PART 3: BRANCH GAUNTLET ---
# Each branch jumps over a "Fail Flag". If any fail, x30 will change.

bne  x1, x2, Pass1   # -1 != 1? True. Jump to Pass1.
addi x30, x0, 1      # ERROR 1: BNE Failed.

Pass1:
blt  x1, x2, Pass2   # -1 < 1? True. Jump to Pass2.
addi x30, x0, 2      # ERROR 2: BLT Failed.

Pass2:
bge  x2, x1, Pass3   # 1 >= -1? True. Jump to Pass3.
addi x30, x0, 3      # ERROR 3: BGE Failed.

Pass3:
bltu x2, x1, Pass4   # 1 < -1 (Unsigned Huge)? True. Jump to Pass4.
addi x30, x0, 4      # ERROR 4: BLTU Failed.

Pass4:
bgeu x1, x2, Finish  # -1 (Huge) >= 1? True. Jump to Finish.
addi x30, x0, 5      # ERROR 5: BGEU Failed.

Finish:
addi x31, x0, 1      # SUCCESS!
beq  x0, x0, Finish  # Stop.